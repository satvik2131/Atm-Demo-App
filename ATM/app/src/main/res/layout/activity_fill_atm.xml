<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/new_bg"
    tools:context=".Admin.FillATMActivity">

    <LinearLayout
        android:id="@+id/bghomeLayout"
        android:layout_width="73dp"
        android:layout_height="44dp"
        android:orientation="horizontal" />

    <Spinner
        android:id="@+id/atmMachineList"
        android:layout_width="183dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="192dp"
        android:layout_marginLeft="192dp"
        android:layout_marginTop="62dp"
        android:background="@android:drawable/btn_dropdown"
        android:spinnerMode="dropdown" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="117dp"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="false"
        android:layout_alignParentBottom="false"
        android:layout_marginStart="23dp"
        android:layout_marginLeft="23dp"
        android:layout_marginTop="171dp"
        android:text="Balance"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="127dp" />

    <TextView
        android:id="@+id/atmBalance"
        android:layout_width="209dp"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="false"
        android:layout_marginStart="171dp"
        android:layout_marginLeft="171dp"
        android:layout_marginTop="168dp"
        android:layout_marginBottom="515dp"
        android:gravity="start"
        android:text="0"
        android:textColor="#050505"
        android:visibility="visible"
        tools:layout_editor_absoluteX="215dp"
        tools:layout_editor_absoluteY="129dp"
        tools:visibility="visible"
        />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="117dp"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="false"
        android:layout_alignParentBottom="false"
        android:layout_marginLeft="23dp"
        android:layout_marginTop="285dp"
        android:text="Fill Amount"
        android:textAlignment="center"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="117dp"
        tools:layout_editor_absoluteY="264dp"
        android:gravity="center_horizontal" />

    <EditText
        android:id="@+id/fillAmount"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="173dp"
        android:layout_marginTop="281dp"
        android:ems="10"
        android:inputType="number"
        tools:layout_editor_absoluteX="198dp"
        tools:layout_editor_absoluteY="254dp" />

    <Button
        android:id="@+id/fillButton"
        android:layout_width="131dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="57dp"
        android:layout_marginTop="377dp"
        android:text="Fill"
        tools:layout_editor_absoluteX="118dp"
        tools:layout_editor_absoluteY="407dp" />

    <Button
        android:id="@+id/cancelButton"
        android:layout_width="131dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="195dp"
        android:layout_marginTop="377dp"
        android:text="Cancel"
        tools:layout_editor_absoluteX="229dp"
        tools:layout_editor_absoluteY="410dp" />

</RelativeLayout>